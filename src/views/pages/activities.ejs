<div>
  <h2>Activity page</h2>
  <table class="table table-dark">
    <thead>
      <tr>
        <th scope="col">Id</th>
        <th scope="col">Created</th>
        <th scope="col">Title</th>
        <th scope="col">Type</th>
      </tr>
    </thead>
    <tbody>
      <% for (let i = 0; i < viewData.events.length; i++) { %>
        <tr>
          <th scope="row"><%= viewData.events[i].id %></th>
          <td><%= viewData.events[i].created_at %></td>
          <td><%= viewData.events[i].target_title %></td>
          <td><%= viewData.events[i].target_type %></td>
        </tr>
      <% } %>
    </tbody>
  </table>
</div>

